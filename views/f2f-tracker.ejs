<div class='modal fade' role='dialog' id='bonusCalcModal'>
  <div class='modal-dialog modal-dialog-centered' role='document' style="max-width: 600px;">
    <div class='modal-content'>
      <div class='modal-body'>
        <div class="form-group">
          <style media="screen">
