    $('#vistaModal').on('shown.bs.modal',function(e){
      if ("<%=job.vistaName%>") {
        $("#vistaName").val("<%=job.vistaName%>").selectpicker('refresh')
        getVistaData('<%=job.vistaName%>').then(gridData=>{
          var oTable = new Tabulator("#vistaTable", {
