      data:{agentID:$('#newAgentName').val(),jobID:jobID},
      type: 'POST',
      success: function (response) {
        table.replaceData().then(function(e){
          $('#allocAdder').modal('hide')
        })
      },
      error: function (jqXHR, exception) {
        alert(exception)
        console.log(exception)
      },
    })
  }
